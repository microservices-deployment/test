node {
    def app
    try {
        
        stage('Checkout')
        {
            git 'https://github.com/microservices-deployment/test.git'
        }
        
        stage('Docker Build')
        {
            app = docker.build
        }
        stage('Push')
        {
            environment {
                 registry = "https://cloud.docker.com/repository/registry-1.docker.io/microservicesdep/test"
                 registryCredential = 'Venkata@3'
                        }
        }
       } catch (err)
        {
        currentBuild.result = 'Failure'
        }
    }
